{
  "name": "@blueeast/bluerain-os",
  "version": "0.4.0",
  "description": "A browser based OS, made with React JS",
  "main": "dist/index.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "test": "jest",
    "coverage": "npm run codecov-coverage && npm run codacy-coverage",
    "codecov-coverage": "codecov",
    "codacy-coverage": "cat ./coverage/lcov.info | ./node_modules/.bin/codacy-coverage",
    "start": " webpack-dev-server --content-base tests/integration/web/hello-world/src --config ./tests/integration/web/hello-world/webpack.config.js --inline --hot --history-api-fallback",
    "demo:todo": " webpack-dev-server --content-base tests/integration/web/todo/src --config ./tests/integration/web/todo/webpack.config.js --inline --hot --history-api-fallback",
    "build": "rm -rf dist && babel -D src --out-dir dist && flow-copy-source src dist",
    "build:prod": "webpack -p && cp src/index.js dist/index.js",
    "lint": "./node_modules/.bin/eslint ./src/",
    "flow": "flow",
    "docs": "./node_modules/.bin/documentation build src/** -f md -o ../../docs/api/reference.md",
    "docs:watch": "./node_modules/.bin/documentation build --watch src/** -f md -o docs/api/reference.md",
    "build:watch": "nodemon --exec \"yarn build\" --ignore dist/"
  },
  "keywords": [
    "reactjs"
  ],
  "author": "Abdul Rehman Talat <rehman.talat@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BlueEastCode/bluerain-os.git"
  },
  "bugs": {
    "url": "https://github.com/BlueEastCode/bluerain-os/issues"
  },
  "homepage": "https://github.com/BlueEastCode/bluerain-os#readme",
  "dependencies": {
    "@blueeast/bluerain-plugin-react-router": "^0.4.0",
    "@blueeast/bluerain-plugin-redux": "^0.4.0",
    "eventemitter3": "^2.0.3",
    "history": "^4.6.3",
    "html-tags": "^2.0.0",
    "is-electron": "^2.1.0",
    "lodash.compose": "^2.4.1",
    "lodash.get": "^4.4.2",
    "lodash.isfunction": "^3.0.8",
    "lodash.isnil": "^4.0.0",
    "lodash.kebabcase": "^4.1.1",
    "lodash.merge": "^4.6.0",
    "lodash.reject": "^4.6.0",
    "lodash.set": "^4.3.2",
    "react": "^16.0.0-alpha.12",
    "react-dom": "^16.0.0-alpha.12",
    "react-redux": "^5.0.6",
    "reactxp": "^0.42.0-rc.25",
    "recompose": "^0.25.0",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "babel-cli": "6.16.0",
    "babel-core": "6.18.0",
    "babel-eslint": "7.0.0",
    "babel-jest": "^17.0.2",
    "babel-loader": "6.2.5",
    "babel-plugin-react-transform": "2.0.2",
    "babel-polyfill": "6.16.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-es2016": "6.16.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "babel-preset-stage-2": "6.18.0",
    "babel-register": "6.18.0",
    "classnames": "^2.2.5",
    "codacy-coverage": "^2.0.2",
    "documentation": "^5.2.1",
    "eslint": "^3.7.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-blueeast": "^0.2.0",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "flow-bin": "^0.53.1",
    "flow-copy-source": "^1.2.1",
    "jest": "^20.0.4",
    "jest-cli": "^20.0.4",
    "json-loader": "^0.5.7",
    "prop-types": "^15.5.10",
    "raf": "^3.3.2",
    "redux-mock-store": "^1.2.3",
    "todomvc-app-css": "^2.1.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "files": [
    "dist"
  ],
  "jest": {
    "testRegex": "\\.test\\.js$",
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**.{js,jsx}"
    ],
    "setupFiles": [
      "raf/polyfill"
    ]
  }
}
