{
  "name": "@blueeast/bluerain-plugin-taskbar",
  "version": "0.4.0",
  "description": "A BlueRain Plugin that integrates taskbar into apps",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "build": "babel -D src --out-dir dist && flow-copy-source src dist",
    "flow": "flow",
    "lint": "eslint ./src/",
    "docs": "documentation readme src/** --section=API"
  },
  "author": "BlueEast Team",
  "license": "MIT",
  "dependencies": {
    "@blueeast/bluerain-os": "^0.4.0",
    "prop-types": "^15.5.10",
    "react": "^16.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.16.0",
    "babel-core": "6.18.0",
    "babel-eslint": "7.0.0",
    "babel-jest": "^17.0.2",
    "babel-loader": "6.2.5",
    "babel-plugin-react-transform": "2.0.2",
    "babel-polyfill": "6.16.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "6.18.0",
    "documentation": "^5.2.1",
    "eslint": "^3.7.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-blueeast": "^0.2.0",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "flow-bin": "^0.53.1",
    "flow-copy-source": "^1.2.1",
    "jest": "^20.0.4",
    "jest-cli": "^20.0.4"
  },
  "files": [
    "dist"
  ],
  "jest": {
    "testRegex": "\\.test\\.js$",
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**.{js,jsx}"
    ]
  }
}
