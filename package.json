{
  "name": "@blueeast/bluerain-os",
  "version": "1.0.1",
  "description": "A browser based OS, made with React JS",
  "keywords": [],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "typings": "dist/types/bluerain-os.d.ts",
  "files": [
    "dist"
  ],
  "author": "BlueEast Team <code@blueeast.com>",
  "repository": {
    "type": "git",
    "url": "http:git@github.com:BlueEastCode/bluerain-os.git",
    "dry-run": false
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "storybook": "start-storybook -p 6006",
    "storybook-native": "storybook start -p 7007",
    "prestorybook-native": "rnstl",
    "build-storybook": "build-storybook",
    "lint": "tslint -t codeFrame 'src/**/*.ts' 'src/**/*.tsx'",
    "prebuild": "rimraf dist",
    "build": "tsc && babel src --ignore src/**/__tests__ --out-dir dist --extensions '.ts,.tsx'",
		"postbuild": "rimraf compiled",
    "start": "tsc -w & rollup -c rollup.config.ts -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:prod": "npm run lint && npm run test -- --coverage --no-cache",
    "deploy-docs": "ts-node tools/gh-pages-publish",
    "docs": "documentation readme src/** --section=API",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "commit": "git-cz",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "semantic-release-prepare": "ts-node tools/semantic-release-prepare",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write --use-tabs --single-quote",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "types": "conventional-commit-types",
      "helpMessage": "Use \"npm run commit\" instead, we use conventional-changelog format :) (https://github.com/commitizen/cz-cli)"
    },
    "react-native-storybook-loader": {
      "searchDir": [
        "./src"
      ],
      "pattern": "**/*.stories.tsx",
      "outputFile": "./storybook/storyLoader.tsx"
    }
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 74,
        "functions": 79,
        "lines": 86,
        "statements": 86
      }
    },
    "collectCoverage": true,
    "mapCoverage": true
  },
  "bundlesize": [
    {
      "path": "./dist/bluerain-os.es5.js",
      "maxSize": "18 kB"
    }
  ],
  "resolutions": {
    "babel-core": "^7.0.0-bridge.0"
  },
  "dependencies": {
    "@blueeast/bluerain-ui-interfaces": "https://github.com/BlueEastCode/bluerain-ui-interfaces.git#fix/removal-of-Api",
    "eventemitter3": "^2.0.3",
    "immutable": "^3.8.2",
    "lodash.flowright": "^3.5.0",
    "lodash.get": "^4.4.2",
    "lodash.isboolean": "^3.0.3",
    "lodash.isnil": "^4.0.0",
    "lodash.kebabcase": "^4.1.1",
    "lodash.merge": "^4.6.0",
    "lodash.set": "^4.3.2",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom-factories": "^1.0.2",
    "recompose": "^0.25.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.32",
    "@babel/core": "^7.0.0-beta.32",
    "@babel/node": "^7.0.0-beta.32",
    "@babel/preset-env": "^7.0.0-beta.32",
    "@babel/preset-react": "^7.0.0-beta.32",
    "@babel/preset-stage-3": "^7.0.0-beta.32",
    "@babel/preset-typescript": "^7.0.0-beta.32",
    "@babel/register": "^7.0.0-beta.32",
    "@blueeast/bluerain-platform-reactxp": "^2.0.0",
    "@blueeast/tslint-config-blueeast": "^0.5.5",
    "@storybook/addon-actions": "^3.3.12",
    "@storybook/addon-links": "^3.3.12",
    "@storybook/react": "^3.3.12",
    "@storybook/react-native": "^3.3.12",
    "@types/lodash.flowright": "^3.5.3",
    "@types/lodash.get": "^4.4.3",
    "@types/lodash.isboolean": "^3.0.3",
    "@types/lodash.isnil": "^4.0.3",
    "@types/lodash.kebabcase": "^4.1.3",
    "@types/lodash.merge": "^4.6.3",
    "@types/lodash.set": "^4.3.3",
    "@types/prop-types": "^15.5.2",
    "@types/react-dom-factories": "^1.0.0",
    "@types/recompose": "^0.24.4",
    "@types/storybook__react": "^3.0.6",
    "awesome-typescript-loader": "^3.4.1",
    "babel-loader": "8.0.0-beta.0",
    "babel-plugin-module-resolver": "^3.0.0",
    "bundlesize": "^0.15.3",
    "codecov": "^2.3.1",
    "colors": "^1.1.2",
    "commitizen": "^2.9.6",
    "coveralls": "^3.0.0",
    "cross-env": "^5.0.1",
    "cz-conventional-changelog": "^2.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "husky": "^0.14.0",
    "jest": "^21.0.0",
    "jest-cli": "^21.2.1",
    "lint-staged": "^4.0.0",
    "lodash.camelcase": "^4.3.0",
    "prettier": "^1.4.4",
    "prompt": "^1.0.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.2.0",
    "react-native-storybook-loader": "^1.7.0",
    "react-test-render": "^1.1.1",
    "redux-mock-store": "^1.3.0",
    "replace-in-file": "^3.0.0-beta.2",
    "rimraf": "^2.6.1",
    "rollup": "^0.50.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-sizes": "^0.4.2",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "semantic-release": "^8.0.0",
    "ts-jest": "^21.0.0",
    "ts-node": "^3.0.6",
    "tslint": "^5.4.3",
    "typedoc": "^0.9.0",
    "typescript": "^2.5.3",
    "validate-commit-msg": "^2.12.2"
  }
}
