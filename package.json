{
	"name": "bluerain-monorepo",
  "version": "0.1.0",
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "clean": "lerna clean --yes",
    "build": "lerna run build",
    "copy-internal-deps": "cd demo/demo-react-native && node ./copy-internal-deps.js && cd ../..",
    "test": "lerna run test",
