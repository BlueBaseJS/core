{
  "name": "bluerain-os",
  "version": "0.1.0",
  "description": "A browser based OS, made with React JS",
  "main": "index.js",
  "scripts": {
    "start": " webpack-dev-server --content-base test/integration/web/src --config ./test/integration/web/webpack.config.js --inline --hot --history-api-fallback",
    "build": "rm -rf dist && babel -D src --out-dir dist",
    "build:prod": "webpack -p && cp src/index.js dist/index.js",
    "lint": "./node_modules/.bin/eslint ./src/"
  },
  "keywords": [
    "reactjs"
  ],
  "author": "Abdul Rehman Talat <rehman.talat@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "lodash.reject": "^4.6.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.1.2",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "babel-cli": "6.16.0",
    "babel-core": "6.18.0",
    "babel-eslint": "7.0.0",
    "babel-jest": "17.0.2",
    "babel-loader": "6.2.5",
    "babel-plugin-react-transform": "2.0.2",
    "babel-polyfill": "6.16.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-es2016": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "babel-preset-stage-2": "6.18.0",
    "babel-register": "6.18.0",
    "eslint": "^3.7.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-blueeast": "^0.2.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
